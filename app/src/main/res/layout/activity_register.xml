<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ddragons.benny.activities.RegisterActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="26dp"
        android:layout_toEndOf="@+id/etPassword"
        android:layout_x="283dp"
        android:layout_y="439dp"
        android:text="OK"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="421dp" />

    <EditText
        android:id="@+id/etFirstName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="35dp"
        android:layout_toStartOf="@+id/button"
        android:layout_x="34dp"
        android:layout_y="140dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Vitaliy"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="102dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/etFirstName"
        android:layout_marginStart="14dp"
        android:layout_marginTop="64dp"
        android:layout_x="31dp"
        android:layout_y="43dp"
        android:text="Setup Your account"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="51dp" />

    <EditText
        android:id="@+id/etEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etPassword"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="39dp"
        android:layout_x="36dp"
        android:layout_y="244dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="abcd@gmail.com"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="234dp" />

    <EditText
        android:id="@+id/etLastName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etFirstName"
        android:layout_marginTop="27dp"
        android:layout_toStartOf="@+id/button"
        android:layout_x="36dp"
        android:layout_y="192dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Tsyapa"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="169dp" />

    <EditText
        android:id="@+id/etPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button"
        android:layout_marginStart="56dp"
        android:layout_marginTop="59dp"
        android:layout_x="42dp"
        android:layout_y="302dp"
        android:ems="10"
        android:inputType="textPassword"
        android:text="Password"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="292dp" />

    <EditText
        android:id="@+id/etPhoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="40dp"
        android:layout_y="364dp"
        android:ems="10"
        android:inputType="phone"
        android:text="0635883612" />

</AbsoluteLayout>
